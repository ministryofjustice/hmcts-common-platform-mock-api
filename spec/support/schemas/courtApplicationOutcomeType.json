{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/core/courts/public/courtApplicationOutcomeType.json",
  "description": "The type of outcome regarding an application",
  "type": "object",
  "properties": {
    "id": {
	  "description": "The identifier of the application outcome type from reference data",
	  "$ref": "courtsDefinitions.json#/definitions/uuid"
	},
	"sequence": {
	  "$ref": "courtsDefinitions.json#/definitions/positiveInteger"
	},
	"description": {
	  "description": "The value of the reference data",
	  "type": "string"
	}
  },
  "required": [
	 "id",
	 "sequence",
	 "description"
  ],
  "additionalProperties": false
}