{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://justice.gov.uk/core/courts/public/defendantHearingYouthMarker.json",
	"description": "For criminal courts a youth marker is set based on a calculation per hearing start date",
	"type": "object",
	"properties": {
		"prosecutionCaseId": {
			"$ref": "courtsDefinitions.json#/definitions/uuid"
		},
		"defendantId": {
			"$ref": "courtsDefinitions.json#/definitions/uuid"
		},
		"hearingId": {
			"$ref": "courtsDefinitions.json#/definitions/uuid"
		},
		"marker": {
			"description": "The youth from reference data",
			"$ref": "marker.json#"
		}
	},
	"required": [
		"prosecutionCaseId",
		"defendantId",
		"hearingId",
		"marker"
	],
	"additionalProperties": false
}