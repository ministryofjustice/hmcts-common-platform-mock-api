{
	 "$schema": "http://json-schema.org/draft-04/schema#",
	 "id": "http://justice.gov.uk/core/courts/public/hearingCaseNote.json",
	 "description": "The prosecution case notes entered at a hearing",
	 "type": "object",
	 "properties": {
		 "originatingHearingId": {
			 "$ref": "courtsDefinitions.json#/definitions/uuid"
		},
		 "id": {
			 "$ref": "courtsDefinitions.json#/definitions/uuid"
		},
		 "courtClerk": {
			 "$ref": "delegatedPowers.json"
		},
		 "prosecutionCases": {
			 "type": "array",
			 "minItems": 1,
			 "items": {
				 "$ref": "courtsDefinitions.json#/definitions/uuid"
			}
		},
		 "noteDateTime": {
			 "type": "string",
			 "format": "date-time"
		},
		 "noteType": {
			 "type": "string",
			 "enum": [
				"HMCTS",
				"LEGAL"
			]
		},
		 "note": {
			 "type": "string"
		}
	},
	 "required": [
		"id",
		"originatingHearingId",
		"prosecutionCases",
		"noteDateTime",
		"noteType",
		"note",
		"courtClerk"
	],
	 "additionalProperties": false
}